{
  "name": "browser-cdp",
  "version": "0.6.3",
  "description": "Browser automation via Chrome DevTools Protocol - control Chrome, Brave, Edge with real browser profiles",
  "type": "module",
  "bin": {
    "browser-cdp": "./cli.js"
  },
  "scripts": {
    "start": "node cli.js",
    "test": "node --test 'tests/*.test.js'",
    "test:unit": "node --test tests/utils.test.js",
    "test:integration": "node --test tests/commands.test.js",
    "test:watch": "node --test --watch tests/"
  },
  "files": [
    "cli.js",
    "src/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpaluy/browser-cdp.git"
  },
  "homepage": "https://github.com/dpaluy/browser-cdp#readme",
  "bugs": {
    "url": "https://github.com/dpaluy/browser-cdp/issues"
  },
  "dependencies": {
    "lighthouse": "^13.0.1",
    "playwright": "^1.49.0"
  },
  "keywords": [
    "browser",
    "automation",
    "cdp",
    "chrome",
    "brave",
    "playwright",
    "devtools"
  ],
  "author": "Majestic Labs",
  "license": "MIT"
}
